<div id="pics" class="transitions-enabled">
  <% @pics.each do |pic| %>
    <div class="box panel panel-default">
      <%= link_to image_tag(pic.image.url(:medium)), pic %>
      <div class="panel-body">
        <% if pic.user == current_user %>
          <div class="actions">
            <%= link_to 'Edit', edit_pic_path(pic) %>
            <%= link_to 'Destroy', pic, method: :delete, data: { confirm: 'Are you sure?' } %>
          </div>
        <% end %>
      </div>
    </div>
  <% end %>
</div>  